<% @form.lieux.each do |lieu| %>
  document.querySelector("#creneaux-lieu-<%= lieu.id %>").outerHTML = "<%= escape_javascript(
    render(
      'creneaux',
      lieu: lieu,
      creneaux: @creneaux_by_lieux[lieu.id],
      agent_creneaux_search_form: @form,
      next_availability: @next_availability_by_lieux[lieu.id]
    )
  ) %>";
<% end %>