= simple_form_for form, url: admin_departement_zone_imports_path(current_departement), as: :zone_import do |f|
  = f.input :zones_file, \
    as: :file, \
    label: "Fichier à importer"
  = f.input :dry_run, \
    as: :boolean, \
    label: "Simulation d'import", \
    hint: "Aucune commune ne sera importée"
  .text-right= f.submit class: "btn btn-primary"
